{
  "name": "@typeform/embed",
  "version": "0.10.0",
  "description": "Library to embed Typeforms in your website",
  "author": "Typeform",
  "repository": {
    "type": "git",
    "url": "https://github.com/Typeform/embed.git"
  },
  "main": "lib/api",
  "files": [
    "assets",
    "lib",
    "dist",
    "README.md"
  ],
  "scripts": {
    "install:token": "echo //registry.npmjs.org/:_authToken=${NPM_TOKEN} > .npmrc",
    "clean": "yarn run clean:dist && yarn run clean:lib",
    "clean:dist": "rm -rf dist",
    "clean:lib": "rm -rf lib",
    "start": "NODE_ENV=development webpack-dev-server -d --config webpack.config.dist.js",
    "test": "yarn run lint && yarn run test:unit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "yarn run lint",
    "prepublish": "yarn run lib",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once --pro",
    "copy": "yarn run copy:assets && yarn run copy:helpcenter && yarn run copy:demo",
    "copy:assets": "copyfiles -f assets/* dist",
    "copy:helpcenter": "copyfiles -f helpcenter/* dist",
    "copy:demo": "copyfiles -f demo/* dist",
    "build": "yarn run dist && yarn run lib && yarn run copy",
    "dist": "yarn run clean:dist && webpack --config webpack.config.dist.js",
    "lib": "yarn run clean:lib && webpack --config webpack.config.lib.js",
    "lint": "eslint ./src --ext .js --ignore-path .eslintignore",
    "deploy": "DEBUG=jarvis jarvis deploy --path dist",
    "deploy:preview": "DEBUG=jarvis jarvis deploy --path dist --preview --notify-preview",
    "coulant:browserstack:start": "export $(cat ./coulant/.env | grep -v '^#' | grep -v '^$' | xargs) && ./coulant/BrowserStackLocal --key $BROWSERSTACK_ACCESS_KEY &",
    "coulant:browserstack:stop": "killall BrowserStackLocal",
    "coulant:chrome": "cd coulant &&  bundle exec cucumber BROWSER=chrome --tags 'not @mobile'",
    "coulant:firefox": "cd coulant && bundle exec cucumber BROWSER=browserstack_firefox --tags 'not @mobile'",
    "coulant:edge": "cd coulant && bundle exec cucumber BROWSER=browserstack_edge --tags 'not @mobile'",
    "coulant:chrome_android6": "cd coulant && bundle exec cucumber -p mobile",
    "coulant:ci": "yarn run coulant:chrome && yarn run coulant:firefox && yarn run coulant:edge && yarn run coulant:chrome_android6"
  },
  "devDependencies": {
    "@typeform/eslint-config": "^1.5.1",
    "@typeform/jarvis": "^9.1.0",
    "babel-core": "^6.5.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-emotion": "^9.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "copyfiles": "^1.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^5.13.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "jsdom": "^11.6.2",
    "react": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "15.4.1",
    "semantic-release": "^12.2.5",
    "snazzy": "^7.0.0",
    "travis-deploy-once": "^4.3.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^3.0.1",
    "create-emotion": "^9.0.2",
    "create-emotion-styled": "^9.0.1",
    "custom-event": "^1.0.1",
    "fetch-jsonp": "^1.0.6",
    "is-url": "^1.2.2",
    "preact": "^8.2.7",
    "preact-compat": "^3.15.0",
    "prop-types": "^15.5.10",
    "scrollbar-width": "^3.1.1",
    "spin.js": "^2.3.2",
    "tinycolor2": "^1.4.1",
    "url-parse": "^1.1.7",
    "webfontloader": "^1.6.27"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7",
              "ie >= 9"
            ],
            "useBuiltIns": true
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "emotion"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(css|gif|png|woff|ttf|svg)$": "identity-obj-proxy"
    },
    "roots": [
      "./src"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "react",
      "react-dom"
    ]
  }
}
